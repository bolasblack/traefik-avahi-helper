{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true
  },
  "exclude": [
    "node_modules",
  ]
}
